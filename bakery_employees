/*cafe employee database*/


CREATE TABLE employees (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	name TEXT,
	empl_lvl INTEGER,
	phone TEXT,
	age INTEGER,
	address TEXT,
	tue_avail TEXT,
	wed_avail TEXT,
	thu_avail TEXT,
	fri_avail TEXT,
	sat_avail TEXT,
	sun_avail TEXT)
	;

