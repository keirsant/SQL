	(5, "trainee", "n", "n", "n")
	;



CREATE TABLE training_levels (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	employee_id INTEGER,
	area TEXT,
	area_lvl INTEGER)
	;

