CREATE TABLE employee_levels (
	id INTEGER,
	position TEXT,
	key TEXT,
	trainer TEXT,
	solo_work TEXT)
	;

INSERT INTO employee_levels VALUES
	(1, "owner", "y", "y", "y"),
